			<title>Richiesta Anticipo per l'Ordine di Missione</title>
			<description>Richiesta Anticipo per l'Ordine di Missione</description>
		</aspect>
		<aspect name="missioni_ordine_attachment:uso_auto_propria">
			<title>Richiesta Utilizzo Auto Propria per l'Ordine di Missione</title>
			<description>Richiesta Utilizzo Auto Propria per l'Ordine di Missione</description>
		</aspect>
		<aspect name="missioni_ordine_attachment:ordine">
			<title>Ordine di Missione</title>
			<description>Ordine di Missione</description>
		</aspect>
		<aspect name="missioni_ordine_attachment:annullamento_ordine">
			<title>Annullamento Ordine di Missione</title>
			<description>Annullamento Ordine di Missione</description>
		</aspect>
	</aspects>
</model>
