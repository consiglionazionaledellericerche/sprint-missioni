<?xml version="1.0" encoding="UTF-8"?>
<!-- Definition of new Model -->
<model name="missioni_ordine:missioni_ordineModel" 
	xmlns="http://www.alfresco.org/model/dictionary/1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.alfresco.org/model/dictionary/1.0 modelSchema.xsd">

	<!-- Optional meta-data about the model -->
	<description>Missioni extension Content Model - Missioni</description>
	<author>Gianfranco Gasparro</author>
	<version>1.0</version>

	<!--
		Imports are required to allow references to definitions in other
		models
	-->
	<imports>
		<!-- Import Alfresco Dictionary Definitions -->
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
	</imports>

	<!-- Introduction of new namespaces defined by this model -->
	<namespaces>
		<namespace uri="http://www.cnr.it/model/missioni/ordine/cmis" prefix="missioni_ordine" />
	</namespaces>

	<types>
		<type name="missioni_ordine:main">
			<title>Dati Generali Ordine Missione</title>
			<description>Dati Generali Ordine Missione</description>
			<parent>cm:folder</parent>
			<properties>
				<property name="missioni_ordine:anno">
					<title>Anno</title>
					<type>d:int</type>
				</property>
				<property name="missioni_ordine:numero">
					<title>Numero Ordine di Missione</title>
					<type>d:long</type>
				</property>
				<property name="missioni_ordine:id">
					<title>ID Ordine di Missione</title>
					<type>d:long</type>
				</property>
				<property name="missioni_ordine:uid">
					<title>Utente Ordine di Missione</title>
					<type>d:text</type>
				</property>
				<property name="missioni_ordine:modulo">
					<title>Modulo Ordine di Missione</title>
					<type>d:text</type>
				</property>
				<property name="missioni_ordine:oggetto">
					<title>Oggetto Ordine di Missione</title>
					<type>d:text</type>
				</property>
				<property name="missioni_ordine:destinazione">
					<title>Destinazione Ordine di Missione</title>
					<type>d:text</type>
				</property>
				<property name="missioni_ordine:note">
					<title>Note Ordine di Missione</title>
					<type>d:text</type>
				</property>
				<property name="missioni_ordine:dataInizio">
					<title>Data Inizio Ordine di Missione</title>
					<type>d:datetime</type>
				</property>
				<property name="missioni_ordine:dataFine">
					<title>Data Fine Ordine di Missione</title>
					<type>d:datetime</type>
				</property>
				<property name="missioni_ordine:dataInserimento">
					<title>Data Inserimento Ordine di Missione</title>
					<type>d:datetime</type>
				</property>
			</properties>
		</type>
	</types>
</model>
